{
  "framework": null,
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install required'",
  "rewrites": [
    {
      "source": "/",
      "destination": "/main.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}